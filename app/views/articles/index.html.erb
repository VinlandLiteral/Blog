
<table> 
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
  
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <%= link_to "Show", article_path(article) %>
      <%= link_to "Edit", edit_article_path(article) %>

    </tr>
  <% end %>
</table>


